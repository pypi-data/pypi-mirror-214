@p2g.must_be(
"( while V[0] < 10:              )",
"N1000",
"  IF [#200 GE 10.] GOTO 1002",
"(     V[0] += 1                 )",
"  #200= #200 + 1.",
"(     continue                  )",
"  GOTO 1000",
"(     V[1] += 9                 )",
"  #201= #201 + 9.",
"  GOTO 1000",
"N1002",
"( V[2] = 7                      )",
"  #202= 7.",
)

def test_continue():
    while V[0] < 10:
        V[0] += 1
        continue
        V[1] += 9
    V[2] = 7
