{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/local/include",
                "/usr/local/lib/wx/include/osx_cocoa-unicode-3.0",
                "/usr/local/include/wx-3.0"
            ],
            "defines": [
                "_FILE_OFFSET_BITS=64",
                "WX_PRECOMP",
                //"__WXMAC__",
                //"__WXOSX__",
                "__WXOSX_COCOA__",
                "__ASSERT_MACROS_DEFINE_VERSIONS_WITHOUT_UNDERSCORES=1",
                "DEBUG"
            ],
            "macFrameworkPath": ["/System/Library/Frameworks"],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "gnu++11",
            "intelliSenseMode": "clang-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        },
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/local/include",
                "/usr/local/include/wx-3.0"
            ],
            "defines": [
                "_FILE_OFFSET_BITS=64",
                "WXUSINGDLL",
                "__WXGTK__",
                "DEBUG"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "gnu++11",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}