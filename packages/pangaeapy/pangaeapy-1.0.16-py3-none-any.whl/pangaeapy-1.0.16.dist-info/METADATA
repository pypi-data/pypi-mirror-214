Requires-Dist: lxml (>=4.9.1)
Requires-Dist: requests (>=2.26.0)
Requires-Dist: pandas (>=1.3.5)
Requires-Dist: numpy (>=1.21.0)
Requires-Dist: netcdf4 (~=1.6.3)

